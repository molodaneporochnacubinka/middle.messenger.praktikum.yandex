.profile_info {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.profile_info_content {
  width: 100%;
  margin: 60px 0;
}

.item {
  display: flex;
  width: 100%;
  height: 32px;
  border-bottom: 1px solid var(--grey-light-2);
  justify-content: space-between;
  align-items: center;
}

.info__name {
  color: var(--title-color);
}
